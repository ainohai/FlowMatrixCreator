{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": false,
    "noImplicitThis": true,
    "module": "ES6",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "experimentalDecorators": true,
    "isolatedModules": true,
    "exactOptionalPropertyTypes":false
  }
}